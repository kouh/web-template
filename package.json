{
  "name": "web-template",
  "description": "## Overview",
  "version": "1.0.0",
  "devDependencies": {
    "babel": "^5.4.7",
    "babel-eslint": "^3.1.17",
    "browser-sync": "^2.7.11",
    "del": "^1.2.0",
    "gulp": "^3.9.0",
    "gulp-autoprefixer": "^2.0.0",
    "gulp-babel": "^5.1.0",
    "gulp-cache": "^0.2.10",
    "gulp-changed": "^1.0.0",
    "gulp-eslint": "^0.14.0",
    "gulp-if": "^1.2.1",
    "gulp-imagemin": "^2.0.0",
    "gulp-load-plugins": "^0.10.0",
    "gulp-minify-css": "^1.1.6",
    "gulp-minify-html": "^1.0.3",
    "gulp-sass": "^2.0.0",
    "gulp-sourcemaps": "^1.3.0",
    "gulp-uglify": "^1.2.0",
    "gulp-concat": "^2.5.2",
    "gulp-useref": "^1.2.0",
    "run-sequence": "^1.1.1"
  },
  "eslintConfig": {
    "parser": "babel-eslint",
    "env": {
      "browser": true,
      "node": true
    },
    "rules": {
      "strict": 2,
      "default-case": 1,
      "no-self-compare": 2,
      "no-else-return": 2,
      "no-throw-literal": 2,
      "no-void": 2,
      "no-alert": 1,
      "max-params": [
        1,
        5
      ],
      "max-depth": [
        1,
        3
      ],
      "max-len": [
        1,
        100,
        2
      ],
      "indent": [
        1,
        2
      ],
      "comma-style": [
        2,
        "last"
      ],
      "brace-style": [
        2,
        "1tbs",
        {
          "allowSingleLine": false
        }
      ],
      "consistent-this": [
        2,
        "DO NOT USE"
      ],
      "space-in-brackets": [
        2,
        "never"
      ],
      "space-before-function-parentheses": 0,
      "space-before-blocks": [
        2,
        "always"
      ],
      "space-after-keywords": [
        2,
        "always"
      ],
      "no-var": 2,
      "no-new-require": 2,
      "no-lonely-if": 2,
      "no-nested-ternary": 2,
      "no-extra-strict": 0,
      "no-multiple-empty-lines": [
        2,
        {
          "max": 1
        }
      ],
      "no-unused-expressions": 0,
      "no-use-before-define": 0,
      "quotes": [
        2,
        "single"
      ],
      "no-multi-spaces": [
        2,
        {
          "exceptions": {
            "Property": true,
            "ImportDeclaration": true,
            "VariableDeclarator": true,
            "AssignmentExpression": true
          }
        }
      ],
      "key-spacing": [
        2,
        {
          "align": "colon",
          "beforeColon": false,
          "afterColon": true
        }
      ]
    }
  }
}
